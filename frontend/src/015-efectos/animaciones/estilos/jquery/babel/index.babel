$(document).ready( () => {
  // Agrandar texto
  function grande() {
    $("#texto").animate({fontSize: "20px"}, 1000);
  }
  
  // EmpequeÃ±ecer texto
  function enano() {
    $("#texto").animate({fontSize: "12px"});
  }
  
  $("#grande").click( () => grande() );
  $("#enano").click( () => enano() );
  
})